# Cafe24 API 설정
NEXT_PUBLIC_CAFE24_MALL_ID=sopexkorea
NEXT_PUBLIC_CAFE24_CLIENT_ID=your_client_id
NEXT_PUBLIC_CAFE24_CLIENT_SECRET=your_client_secret
NEXT_PUBLIC_CAFE24_REDIRECT_URI=https://spx-price.vercel.app/api/auth/callback

# Firebase Admin SDK 설정 (서버 사이드 전용)
# Firebase Console > Project Settings > Service Accounts > Generate new private key
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-xxxxx@your-project.iam.gserviceaccount.com
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour_Private_Key_Here\n-----END PRIVATE KEY-----\n"

# 관리자 로그인 설정 (실제 .env.local에만 설정)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_secure_password